  <sql id="Vo_Where_Clause" >
    <!-- <if test="queryBeanField != null" > -->
    <!-- and dbField = #{queryBeanField,jdbcType=dbFieldStyle} -->
    <!-- </if> -->
     <if test="userId != null" >
  		  and u.user_id = #{userId,jdbcType=INTEGER}
    </if>
  </sql>
  <sql id="Table_Join_Clause" >
    <!-- left join tableName tn on t.primaryKey=tn.foreignKey -->
	    <if test="userId != null" >
			left join t_user_role ur  on t.id = ur.role_id
			left join t_users u  on ur.user_id = u.user_id
		</if>
  </sql>
